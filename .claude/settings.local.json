{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm run build:*)",
      "Bash(python manage.py makemigrations:*)",
      "Bash(venvScripts:*)",
      "Bash(\"venv\\Scripts\\activate.bat\")",
      "Bash(./venv/Scripts/python.exe manage.py makemigrations core --name add_status_fields)",
      "Bash(./venv/Scripts/python.exe manage.py migrate)",
      "Bash(./venv/Scripts/python.exe manage.py check)",
      "Bash(npm run lint)",
      "Bash(rm:*)"
    ],
    "deny": []
  }
}